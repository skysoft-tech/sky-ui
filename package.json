{
  "name": "sky-ui",
  "version": "0.0.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "license": "MIT",
  "scripts": {
    "start:dev-app": "nx serve dev-app",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all",
    "test": "nx test",
    "test:coverage": "npm run test -- --coverage",
    "test:all": "nx run-many --target=test --all",
    "test:coverage:all": "nx run-many --target=test --all --coverage",
    "affected:test": "nx affected:test",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "format": "nx format:write",
    "generate": "nx g @nx/angular:library %npm_config_name% --buildable --publishable --import-path=@sky-ui/%npm_config_name% --prefix=sky",
    "inline-svg": "node projects/icons/src/lib/inline-icons.js projects/icons/src/lib/images projects/icons/src/lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.9",
    "@angular/cdk": "17.0.5",
    "@angular/common": "17.0.9",
