{
  "name": "dev-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "examples/dev-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@ng-doc/builder:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/examples/dev-app",
        "index": "examples/dev-app/src/index.html",
        "browser": "examples/dev-app/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "examples/dev-app/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "node_modules/@ng-doc/app/assets",
            "output": "assets/ng-doc/app"
          },
          {
            "glob": "**/*",
            "input": "node_modules/@ng-doc/ui-kit/assets",
            "output": "assets/ng-doc/ui-kit"
          },
          {
            "glob": "**/*",
            "input": "ng-doc/dev-app/assets",
            "output": "assets/ng-doc"
          },
          {
            "glob": "**/*",
            "input": "node_modules/@ng-doc/app/assets",
            "output": "assets/ng-doc/app"
          },
          {
            "glob": "**/*",
            "input": "node_modules/@ng-doc/ui-kit/assets",
            "output": "assets/ng-doc/ui-kit"
          },
          {
            "glob": "**/*",
            "input": "ng-doc/dev-app/assets",
            "output": "assets/ng-doc"
          },
          "examples/dev-app/src/favicon.ico",
          "examples/dev-app/src/assets"
        ],
        "styles": [
          "node_modules/@ng-doc/app/styles/global.css",
          "examples/dev-app/src/styles.scss"
        ],
        "scripts": [],
        "stylePreprocessorOptions": {
          "includePaths": [
            "projects/theme/src/lib", "."
          ]
        },
        "allowedCommonJsDependencies": [
          "@ng-doc/core"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@ng-doc/builder:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "dev-app:build:production"
        },
        "development": {
          "buildTarget": "dev-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "dev-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "examples/dev-app/jest.config.ts"
      }
    }
  }
}